<Project>
  <PropertyGroup>
    <MicrosoftVisualStudioPackageNext>17.0.31902.203</MicrosoftVisualStudioPackageNext>
    <MicrosoftSqlServerCompactPackageVersion>4.08876.1</MicrosoftSqlServerCompactPackageVersion>
    <MicrosoftVisualStudioComponentModelHostPackageVersion>17.0.439-preview</MicrosoftVisualStudioComponentModelHostPackageVersion>
    <MicrosoftVisualStudioExtensibilityHostingPackageVersion>17.0.32-g9d6b2c6f26</MicrosoftVisualStudioExtensibilityHostingPackageVersion>
    <MicrosoftVisualStudioPackageCurrent>17.0.31723.112</MicrosoftVisualStudioPackageCurrent>
    <MicrosoftVisualStudioDataCorePackageVersion>$(MicrosoftVisualStudioPackageCurrent)</MicrosoftVisualStudioDataCorePackageVersion>
    <MicrosoftVisualStudioDataDesignCommonPackageVersion>$(MicrosoftVisualStudioPackageCurrent)</MicrosoftVisualStudioDataDesignCommonPackageVersion>
    <MicrosoftVisualStudioModelingPackageVersion>$(MicrosoftVisualStudioPackageNext)</MicrosoftVisualStudioModelingPackageVersion>
    <MicrosoftVisualStudioInteropPackageVersion>$(MicrosoftVisualStudioPackageNext)</MicrosoftVisualStudioInteropPackageVersion>
    <MicrosoftVisualStudioTemplateWizardInterfacePackageVersion>$(MicrosoftVisualStudioPackageNext)</MicrosoftVisualStudioTemplateWizardInterfacePackageVersion>
    <MicrosoftVisualStudioTextTemplatingInterfaces100PackageVersion>$(MicrosoftVisualStudioPackageNext)</MicrosoftVisualStudioTextTemplatingInterfaces100PackageVersion>
    <MicrosoftVisualStudioValidationPackageVersion>17.0.1-alpha</MicrosoftVisualStudioValidationPackageVersion>
    <MicrosoftVisualStudioThreadingPackageVersion>17.0.63</MicrosoftVisualStudioThreadingPackageVersion>
    <MicrosoftVSDesignerPackageVersion>$(MicrosoftVisualStudioPackageNext)</MicrosoftVSDesignerPackageVersion>
    <MicrosoftVSSDKBuildToolsPackageVersion>17.0.5234</MicrosoftVSSDKBuildToolsPackageVersion>
    <MicrosoftXmlEditorPackageVersion>$(MicrosoftVisualStudioPackageCurrent)</MicrosoftXmlEditorPackageVersion>
    <MoqPackageVersion>4.0.10827</MoqPackageVersion>
    <MySqlDataEntityPackageVersion>6.7.2-beta-ef6</MySqlDataEntityPackageVersion>
    <SystemDataSqlServerCePackageVersion>4.0.8876.1</SystemDataSqlServerCePackageVersion>
    <VsWebSiteInteropPackageVersion>$(MicrosoftVisualStudioPackageCurrent)</VsWebSiteInteropPackageVersion>
    <XunitPackageVersion>1.9.2</XunitPackageVersion>
  </PropertyGroup>

  <PropertyGroup>
    <!-- During the conversion to SDK projects, continue to list all project items explicitly -->
    <EnableDefaultItems>false</EnableDefaultItems>

    <!-- Assembly info is included explicitly -->
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>

    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <PropertyGroup>
    <SignAssembly>true</SignAssembly>
    <DelaySign>true</DelaySign>
    <!-- AssemblyOriginatorKeyFile is defined in Directory.Build.targets since it depends on properties defined in EntityFramework.settings.targets. -->
  </PropertyGroup>
</Project>
